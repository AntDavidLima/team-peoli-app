{
  "expo": {
    "name": "Team Peoli",
    "slug": "team-peoli",
    "privacy": "public",
    "backgroundColor": "#1A202C",
    "version": "0.4.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-without-text.png",
    "scheme": "team-peoli",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#1A202C"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "androidStatusBar": {
      "backgroundColor": "#000000",
      "barStyle": "light-content"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-without-text.png",
        "backgroundColor": "#0075B7"
      },
      "package": "com.davidlima.teampeoli",
      "blockedPermissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "versionCode": 16
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo-without-text.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "É necessário sua permissão para acessar suas fotos."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d551ce7d-9ead-403b-b87c-86ed44668b8d"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/d551ce7d-9ead-403b-b87c-86ed44668b8d"
    }
  }
}
